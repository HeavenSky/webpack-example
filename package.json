{
	"name": "eslint-es6",
	"version": "0.0.1",
	"license": "MIT",
	"main": "index.js",
	"author": "HeavenSky",
	"description": "eslint check es6 react vue",
	"scripts": {
		"start": "eslint ./ --cache --fix --ext .js,.jsx,.vue"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{js,jsx,vue}": [
			"prettier --write",
			"eslint --cache --fix",
			"git add -f"
		],
		"*.{ts,md,gql,css,less,scss,html,json,flow,yaml,graphql}": [
			"prettier --write",
			"git add -f"
		]
	},
	"prettier": {
		"endOfLine": "lf",
		"htmlWhitespaceSensitivity": "ignore",
		"jsxSingleQuote": false,
		"printWidth": 80,
		"singleQuote": false,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	},
	"devDependencies": {},
	"dependencies": {
		"@commitlint/cli": "^8.1.0",
		"@commitlint/config-conventional": "^8.1.0",
		"eslint": "^6.2.2",
		"eslint-config-ali": "^8.0.2",
		"eslint-config-egg": "^7.5.0",
		"eslint-config-react-app": "^5.0.1",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-flowtype": "^4.2.0",
		"eslint-plugin-react-hooks": "^2.0.1",
		"eslint-plugin-vue": "^5.2.3",
		"mime-types": "^2.1.24",
		"prettier": "^1.18.2",
		"tinify": "^1.6.0-beta.2"
	}
}