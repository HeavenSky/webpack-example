{
	"name": "webpack4-vue",
	"version": "0.0.1",
	"license": "MIT",
	"main": "index.js",
	"author": "HeavenSky",
	"description": "study vue ele",
	"scripts": {
		"dll": "cross-env NODE_ENV=development webpack --config webpack.dll.js --cache --colors --profile --progress --hide-modules",
		"dev": "cross-env NODE_ENV=development webpack --cache --colors --profile --progress --hide-modules",
		"dst": "cross-env NODE_ENV=production webpack --cache --colors --profile --progress --hide-modules",
		"app": "cross-env NODE_ENV=production webpack --config webpack.dll.js --cache --colors --profile --progress --hide-modules && cross-env NODE_ENV=production webpack --cache --colors --profile --progress --hide-modules",
		"start": "cross-env NODE_ENV=development webpack-dev-server --cache --colors --profile --progress -d"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,vue,json,scss,less,css}": [
			"prettier --write",
			"git add"
		]
	},
	"prettier": {
		"trailingComma": "es5",
		"printWidth": 128,
		"useTabs": true,
		"tabWidth": 4
	},
	"devDependencies": {
		"autoprefixer": "^9.1.3",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.5",
		"babel-plugin-component": "^1.1.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-0": "^6.24.1",
		"babel-preset-vue": "^2.0.2",
		"clean-webpack-plugin": "^0.1.19",
		"copy-webpack-plugin": "^4.5.2",
		"cross-env": "^5.2.0",
		"css-loader": "^1.0.0",
		"cssnano": "^4.1.0",
		"file-loader": "^2.0.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "next",
		"less": "^3.8.1",
		"less-loader": "^4.1.0",
		"lint-staged": "^7.2.2",
		"mini-css-extract-plugin": "^0.4.2",
		"node-sass": "^4.9.3",
		"postcss-loader": "^3.0.0",
		"prettier": "^1.14.2",
		"promise-loader": "^1.0.0",
		"sass-loader": "7.1.0",
		"uglifyjs-webpack-plugin": "^1.3.0",
		"url-loader": "^1.1.1",
		"vue-loader": "^15.4.1",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.5.17",
		"webpack": "^4.17.1",
		"webpack-cli": "^3.1.0",
		"webpack-dev-server": "^3.1.7",
		"webpack-merge": "^4.1.4"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"element-ui": "^2.4.6",
		"js-cookie": "^2.2.0",
		"moment": "^2.22.2",
		"nprogress": "^0.2.0",
		"numeral": "^2.0.6",
		"pace": "HeavenSky/pace",
		"pubsub-js": "^1.6.1",
		"signals": "^1.0.0",
		"vue": "^2.5.17",
		"vue-router": "^3.0.1",
		"vuex": "^3.0.1"
	}
}