{
	"name": "webpack4-vue",
	"version": "0.0.1",
	"license": "MIT",
	"main": "index.js",
	"author": "HeavenSky",
	"description": "study vue ele",
	"scripts": {
		"bin": "webpack --cache --colors --profile --progress --hide-modules",
		"svc": "webpack-dev-server --cache --colors --profile --progress",
		"start": "cross-env NODE_ENV=development npm run svc",
		"dev": "cross-env NODE_ENV=development npm run bin",
		"app": "cross-env NODE_ENV=production npm run bin",
		"debug": "node --inspect-brk=7777 app.js",
		"report": "npm run app --report",
		"dll": "echo No DllPlugin!!!",
		"log": "standard-version",
		"env": "cross-env"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{js,jsx,vue}": [
			"prettier --write",
			"eslint --cache --fix",
			"git add -f"
		],
		"*.{ts,md,gql,css,less,scss,html,json,flow,yaml,graphql}": [
			"prettier --write",
			"git add -f"
		]
	},
	"prettier": {
		"endOfLine": "lf",
		"htmlWhitespaceSensitivity": "ignore",
		"jsxSingleQuote": false,
		"singleQuote": false,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	},
	"devDependencies": {},
	"dependencies": {
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-do-expressions": "^7.5.0",
		"@babel/plugin-proposal-export-default-from": "^7.5.2",
		"@babel/plugin-proposal-export-namespace-from": "^7.5.2",
		"@babel/plugin-proposal-function-bind": "^7.2.0",
		"@babel/plugin-proposal-function-sent": "^7.5.0",
		"@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
		"@babel/plugin-proposal-numeric-separator": "^7.2.0",
		"@babel/plugin-proposal-optional-chaining": "^7.2.0",
		"@babel/plugin-proposal-pipeline-operator": "^7.5.0",
		"@babel/plugin-proposal-throw-expressions": "^7.2.0",
		"@babel/plugin-syntax-import-meta": "^7.2.0",
		"@commitlint/cli": "^8.1.0",
		"@commitlint/config-conventional": "^8.1.0",
		"@vue/babel-preset-app": "^3.11.0",
		"autoprefixer": "^9.6.1",
		"axios": "^0.19.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-component": "^1.1.1",
		"babel-plugin-import": "^1.12.0",
		"cookie-parser": "^1.4.4",
		"copy-webpack-plugin": "^5.0.4",
		"cross-env": "^5.2.0",
		"css-loader": "^3.2.0",
		"cssnano": "^4.1.10",
		"element-ui": "^2.11.1",
		"eslint": "^6.2.2",
		"eslint-config-ali": "^8.0.2",
		"eslint-config-egg": "^7.5.0",
		"eslint-config-react-app": "^5.0.1",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-flowtype": "^4.2.0",
		"eslint-plugin-react-hooks": "^2.0.1",
		"eslint-plugin-vue": "^5.2.3",
		"esm": "^3.2.25",
		"file-loader": "^4.2.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^3.0.4",
		"js-cookie": "^2.2.1",
		"less": "^3.10.3",
		"less-loader": "^5.0.0",
		"lint-staged": "^9.2.4",
		"mini-css-extract-plugin": "^0.8.0",
		"moment": "^2.24.0",
		"multer": "^1.4.2",
		"node-sass": "^4.12.0",
		"nprogress": "^0.2.0",
		"pace-less": "^1.0.2",
		"postcss-loader": "^3.0.0",
		"postcss-plugin-px2rem": "^0.8.1",
		"prettier": "^1.18.2",
		"sass-loader": "^7.3.1",
		"shelljs": "^0.8.3",
		"standard-version": "^7.0.0",
		"url-loader": "^2.1.0",
		"vant": "^2.1.7",
		"vue": "^2.6.10",
		"vue-loader": "^15.7.1",
		"vue-router": "^3.1.2",
		"vue-template-compiler": "^2.6.10",
		"vuex": "^3.1.1",
		"webpack": "^4.39.2",
		"webpack-bundle-analyzer": "^3.4.1",
		"webpack-cli": "^3.3.7",
		"webpack-dev-server": "^3.8.0",
		"webpack-merge": "^4.2.1"
	}
}