{
	"name": "webpack4-vue",
	"version": "1.0.0",
	"license": "MIT",
	"main": "index.js",
	"author": "HeavenSky",
	"description": "study vue ele",
	"scripts": {
		"log": "standard-version",
		"svc": "webpack-dev-server --cache --colors --profile --progress -d",
		"bin": "webpack --cache --colors --profile --progress --hide-modules",
		"prl": "cross-env NODE_ENV=production npm run bin -- --config config/opt.dll.js",
		"dll": "cross-env NODE_ENV=development npm run bin -- --config config/opt.dll.js",
		"prv": "cross-env NODE_ENV=production npm run bin",
		"dev": "cross-env NODE_ENV=development npm run bin",
		"start": "cross-env NODE_ENV=development npm run svc",
		"app": "npm run prl && npm run prv"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"src/**/*.{js,jsx,vue,json,scss,less,css}": [
			"prettier --write",
			"git add"
		]
	},
	"prettier": {
		"trailingComma": "es5",
		"printWidth": 99,
		"useTabs": true,
		"tabWidth": 2
	},
	"devDependencies": {},
	"dependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-decorators": "^7.4.4",
		"@babel/plugin-proposal-do-expressions": "^7.2.0",
		"@babel/plugin-proposal-export-default-from": "^7.2.0",
		"@babel/plugin-proposal-export-namespace-from": "^7.2.0",
		"@babel/plugin-proposal-function-bind": "^7.2.0",
		"@babel/plugin-proposal-function-sent": "^7.2.0",
		"@babel/plugin-proposal-json-strings": "^7.2.0",
		"@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
		"@babel/plugin-proposal-numeric-separator": "^7.2.0",
		"@babel/plugin-proposal-optional-chaining": "^7.2.0",
		"@babel/plugin-proposal-pipeline-operator": "^7.3.2",
		"@babel/plugin-proposal-throw-expressions": "^7.2.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-syntax-import-meta": "^7.2.0",
		"@babel/plugin-transform-runtime": "^7.4.4",
		"@babel/polyfill": "^7.4.4",
		"@babel/runtime": "^7.4.5",
		"@commitlint/cli": "^8.0.0",
		"@commitlint/config-conventional": "^8.0.0",
		"@vue/babel-preset-app": "^3.8.0",
		"autoprefixer": "^9.6.0",
		"axios": "^0.19.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-component": "^1.1.1",
		"babel-plugin-import": "^1.12.0",
		"clean-webpack-plugin": "^3.0.0",
		"cookie-parser": "^1.4.4",
		"copy-webpack-plugin": "^5.0.3",
		"cross-env": "^5.2.0",
		"css-loader": "^2.1.1",
		"cssnano": "^4.1.10",
		"element-ui": "^2.9.1",
		"esm": "^3.2.25",
		"file-loader": "^4.0.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^2.4.0",
		"js-cookie": "^2.2.0",
		"less": "^3.9.0",
		"less-loader": "^5.0.0",
		"lint-staged": "^8.2.0",
		"mini-css-extract-plugin": "^0.7.0",
		"moment": "^2.24.0",
		"multer": "^1.4.1",
		"node-sass": "^4.12.0",
		"nprogress": "^0.2.0",
		"numeral": "^2.0.6",
		"pace-less": "^1.0.2",
		"postcss-loader": "^3.0.0",
		"postcss-px-to-viewport": "^1.1.0",
		"prettier": "^1.18.2",
		"pubsub-js": "^1.7.0",
