{
	"name": "webpack1-ie8",
	"version": "0.0.1",
	"license": "MIT",
	"main": "index.js",
	"author": "HeavenSky",
	"description": "study react antd ie8",
	"scripts": {
		"log": "standard-version",
		"svc": "webpack-dev-server --cache --colors --profile --progress -d",
		"bin": "webpack --cache --colors --profile --progress --hide-modules",
		"app": "cross-env NODE_ENV=production npm run bin",
		"dev": "cross-env NODE_ENV=development npm run bin",
		"start": "cross-env NODE_ENV=development npm run svc"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"src/**/*.{js,jsx,vue,json,scss,less,css}": [
			"prettier --write",
			"git add"
		]
	},
	"prettier": {
		"trailingComma": "es5",
		"printWidth": 99,
		"useTabs": true,
		"tabWidth": 2
	},
	"devDependencies": {
		"antd": "1.11.6",
		"babel-loader": "6.4.1",
		"clean-webpack-plugin": "1.0.1",
		"copy-webpack-plugin": "4.6.0",
		"extract-text-webpack-plugin": "1.0.1",
		"file-loader": "2.0.0",
		"less-loader": "4.1.0",
		"react": "0.14.9",
		"react-dom": "0.14.9",
		"react-hot-loader": "4.3.12",
		"react-redux": "5.1.1",
		"uglifyjs-webpack-plugin": "1.3.0",
		"webpack": "1.15.0",
		"webpack-dev-server": "1.16.5"
	},
	"dependencies": {
		"@commitlint/cli": "^7.5.2",
		"@commitlint/config-conventional": "^7.5.0",
		"autoprefixer": "^9.5.1",
		"axios": "^0.18.0",
		"babel-core": "^6.26.3",
		"babel-plugin-component": "^1.1.1",
		"babel-plugin-import": "^1.11.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"cookie-parser": "^1.4.4",
		"cross-env": "^5.2.0",
		"css-loader": "^2.1.1",
		"cssnano": "^4.1.10",
		"es3ify-webpack-plugin": "^0.1.0",
		"esm": "^3.2.22",
		"export-from-ie8": "^1.0.5",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"history": "^4.9.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^2.2.0",
		"js-cookie": "^2.2.0",
		"less": "^3.9.0",
		"lint-staged": "^8.1.5",
		"media-match": "^2.0.3",
		"moment": "^2.24.0",
		"multer": "^1.4.1",
		"node-sass": "^4.12.0",
		"nprogress": "^0.2.0",
		"numeral": "^2.0.6",
		"pace-less": "^1.0.2",
		"postcss-loader": "^3.0.0",
		"postcss-px-to-viewport": "^1.1.0",
		"prettier": "^1.17.0",
		"promise-loader": "^1.0.0",
		"pubsub-js": "^1.7.0",
