{
	"name": "webpack3-vue",
	"version": "0.0.1",
	"license": "MIT",
	"main": "index.js",
	"author": "HeavenSky",
	"description": "study vue ele",
	"scripts": {
		"log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
		"app": "cross-env NODE_ENV=production webpack --config webpack.dll.js --cache --colors --profile --progress --hide-modules && cross-env NODE_ENV=production webpack --cache --colors --profile --progress --hide-modules",
		"dst": "cross-env NODE_ENV=production webpack --cache --colors --profile --progress --hide-modules",
		"dll": "cross-env NODE_ENV=development webpack --config webpack.dll.js --cache --colors --profile --progress --hide-modules",
		"dev": "cross-env NODE_ENV=development webpack --cache --colors --profile --progress --hide-modules",
		"start": "cross-env NODE_ENV=development webpack-dev-server --cache --colors --profile --progress -d"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		]
	},
	"lint-staged": {
		"src/**/*.{js,jsx,vue,json,scss,less,css}": [
			"prettier --write",
			"git add"
		]
	},
	"prettier": {
		"trailingComma": "es5",
		"printWidth": 128,
		"useTabs": true,
		"tabWidth": 4
	},
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-proposal-decorators": "^7.1.2",
		"@babel/plugin-proposal-do-expressions": "^7.0.0",
		"@babel/plugin-proposal-export-default-from": "^7.0.0",
		"@babel/plugin-proposal-export-namespace-from": "^7.0.0",
		"@babel/plugin-proposal-function-bind": "^7.0.0",
		"@babel/plugin-proposal-function-sent": "^7.1.0",
		"@babel/plugin-proposal-json-strings": "^7.0.0",
		"@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
		"@babel/plugin-proposal-numeric-separator": "^7.0.0",
		"@babel/plugin-proposal-optional-chaining": "^7.0.0",
		"@babel/plugin-proposal-pipeline-operator": "^7.0.0",
		"@babel/plugin-proposal-throw-expressions": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-syntax-import-meta": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.1.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.1.0",
		"@babel/runtime": "^7.1.2",
		"@commitlint/cli": "^7.2.0",
		"@commitlint/config-angular": "^7.1.2",
		"autoprefixer": "^9.1.5",
		"babel-loader": "^8.0.4",
		"babel-plugin-component": "^1.1.1",
		"babel-preset-vue": "^2.0.2",
		"body-parser": "^1.18.3",
		"chokidar": "^2.0.4",
		"clean-webpack-plugin": "^0.1.19",
		"conventional-changelog-cli": "^2.0.5",
		"copy-webpack-plugin": "^4.5.2",
		"cross-env": "^5.2.0",
		"css-loader": "^1.0.0",
		"cssnano": "^4.1.4",
		"esm": "^3.0.84",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^2.0.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^3.2.0",
		"http-proxy-middleware": "^0.19.0",
		"husky": "^1.1.1",
		"less": "^3.8.1",
		"less-loader": "^4.1.0",
		"lint-staged": "^7.3.0",
		"multer": "^1.4.0",
		"node-sass": "^4.9.3",
		"path-to-regexp": "^2.4.0",
		"postcss-loader": "^3.0.0",
		"prettier": "^1.14.3",
		"promise-loader": "^1.0.0",
		"sass-loader": "7.1.0",
		"uglifyjs-webpack-plugin": "^1.3.0",
		"url-loader": "^1.1.1",
		"vue-loader": "^15.4.2",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.5.17",
		"webpack": "^3.12.0",
		"webpack-dev-server": "^2.11.3",
		"webpack-merge": "^4.1.4"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"element-ui": "^2.4.8",
		"js-cookie": "^2.2.0",
		"moment": "^2.22.2",
		"nprogress": "^0.2.0",
		"numeral": "^2.0.6",
		"pace": "HeavenSky/pace",
		"pubsub-js": "^1.7.0",
		"signals": "^1.0.0",
		"vue": "^2.5.17",
		"vue-router": "^3.0.1",
		"vuex": "^3.0.1"
	}
}